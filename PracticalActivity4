
package practicalactivity4;
import java.util.Arrays;
import java.util.Scanner;

public class PracticalActivity4 {

    
    public static void main(String[] args) {
    Scanner input = new Scanner(System.in);
    
    String[] candidates = new String[0];
    int [] votes = new int[0];
    int indexes = 0;
    String UserInput = input.nextLine();
    
    
    for(int i = 0; i <= (candidates.length+1); i++){
        
    
    if((UserInput)==(candidates[indexes])){
        votes[indexes] = votes[indexes] + 1;
        
    }
    
    else{
        candidates[indexes] = parseString(UserInput());
            votes[indexes]= (votes[indexes]+1);
    }
    
    }   
    System.out.print(Arrays.toString(candidates));
    System.out.print(Arrays.toString(votes));
 
        
    }

   
    }
    
