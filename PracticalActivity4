
package practicalactivity4;
import java.util.Arrays;
import java.util.Scanner;

public class PracticalActivity4 {

    
    public static void main(String[] args) {
    Scanner input = new Scanner(System.in);
    
    String[] candidates = new String[5];
    
     for (int i = 0; i < candidates.length; i++){
        System.out.println("Please enter the candidates names.");
        candidates[i] = input.nextLine();
    }
    
     System.out.print(Arrays.toString(candidates));

    }
    
}
